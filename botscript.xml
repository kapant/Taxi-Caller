<context> <!-- A regular context -->
    <input pattern = "(привет) *" id = "hi/hello">
		<context id = "greetings">
			<output value = "Привет!"/>
		</context>
    </input>
    <input pattern = "(как) * (дела) *" id = "feels">
		<context id = "feeling">
			<output value = "Все норм)"/>
		</context>
    </input>
	<input pattern = "(который) * (час) *">
		<var name = "Time">
			<script>
				var date = new Date();
				var time = date.getHours();
				time += ":" + date.getMinutes();
				time;
			</script>
		</var>
		<output value = "Сейчас $Time"/>
	</input>
	<input pattern = "(найди)$Text">
		<var name = "Mark" value = "$Text"/>
		<var name = "Address">
			<script src = "https://api-maps.yandex.ru/2.1/?lang=ru_RU" type = "text/javascript"></script>
			<script>
				ymaps.ready();
			</script>
		</var>
		<output value = "$Address"/>
	</input>
</context>
